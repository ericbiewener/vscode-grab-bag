{
    "name": "grab-bag",
    "displayName": "Grab Bag",
    "description": "Misc",
    "version": "0.0.1",
    "publisher": "edb",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./src/index.js",
    "contributes": {
        "commands": [
            {
                "command": "grabBag.hideTests",
                "title": "Hide Tests"
            },
            {
                "command": "grabBag.showTests",
                "title": "Show Tests"
            },
            {
                "command": "grabBag.gotoSymbolGrouped",
                "title": "Go to Symbol in File (Grouped)..."
            },
            {
                "command": "grabBag.openCorrespondingTestFile",
                "title": "Open Corresponding Test File"
            },
            {
                "command": "grabBag.openCorrespondingSnapshot",
                "title": "Open Corresponding Snapshot"
            },
            {
                "command": "grabBag.jestWatchActiveFile",
                "title": "Jest Watch Active File"
            },
            {
                "command": "grabBag.jestUpdateActiveFile",
                "title": "Jest Update Active File"
            },
            {
                "command": "grabBag.moveEditorToOtherGroup",
                "title": "Move Editor to Other Group"
            },
            {
                "command": "grabBag.toggleEditorMaxWidth",
                "title": "Toggle Editor Max Width"
            },
            {
                "command": "grabBag.flowStatus",
                "title": "Flow Status"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "eslint": "^4.11.0",
        "eslint-config-ericbiewener": "^1.0.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "lodash": "^4.17.5"
    }
}
