{
    "name": "grab-bag",
    "displayName": "Grab Bag",
    "description": "Misc",
    "version": "0.0.1",
    "publisher": "edb",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./src/index.js",
    "contributes": {
        "commands": [
            {
                "title": "Go to Symbol in File (Grouped)...",
                "command": "grabBag.gotoSymbolGrouped"
            },
            {
                "title": "Open Corresponding Test File and Run Suite",
                "command": "grabBag.openCorrespondingTestFile"
            },
            {
                "title": "Open Corresponding Test File and Copy Jest Test Path",
                "command": "grabBag.openCorrespondingTestFileCopyOnly"
            },
            {
                "title": "Open Corresponding Snapshot",
                "command": "grabBag.openCorrespondingSnapshot"
            },
            {
                "title": "Jest Watch Active File",
                "command": "grabBag.jestWatchActiveFile"
            },
            {
                "title": "Jest Update Active File",
                "command": "grabBag.jestUpdateActiveFile"
            },
            {
                "title": "Copy Jest Watch Path",
                "command": "grabBag.copyJestWatchPath"
            },
            {
                "title": "Python Test Active Function",
                "command": "grabBag.pythonTestActiveFunction"
            },
            {
                "title": "Move Editor to Other Group",
                "command": "grabBag.moveEditorToOtherGroup"
            },
            {
                "title": "Toggle Editor Max Width",
                "command": "grabBag.toggleEditorMaxWidth"
            },
            {
                "title": "Move Caret Down 10 Lines",
                "command": "grabBag.moveCaretDown"
            },
            {
                "title": "Move Caret Up 10 Lines",
                "command": "grabBag.moveCaretUp"
            },
            {
                "title": "Copy Escaped File Path",
                "command": "grabBag.copyEscapedFilePath"
            },
            {
                "title": "Copy Python Test Path",
                "command": "grabBag.copyPythonTestPath"
            },
            {
                "title": "Close All Panels",
                "command": "grabBag.closeAllPanels"
            },
            {
                "title": "Focus Open Editor",
                "command": "grabBag.focusOpenEditor"
            },
            {
                "title": "Repeat Last Terminal Command",
                "command": "grabBag.repeatLastTerminalCmd"
            },
            {
                "title": "Mirror File",
                "command": "grabBag.mirrorFile"
            },
            {
                "title": "Better Go To Symbol",
                "command": "grabBag.betterGoToSymbol"
            },
            {
                "title": "Remove Unused Variable Destructuring",
                "command": "grabBag.removeUnusedVariableDestructuring"
            },
            {
                "title": "Move Cursor After Token",
                "command": "grabBag.moveCursorAfterToken"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "prettier": "prettier-eslint --prettier-last --write \"**/*.js\"",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "5.2.6",
        "@types/node": "12.0.1",
        "eslint": "5.16.0",
        "eslint-config-ericbiewener": "3.6.1",
        "husky": "2.3.0",
        "lint-staged": "8.1.6",
        "prettier-eslint-cli": "^4.7.1",
        "vscode": "1.1.34"
    },
    "dependencies": {
        "clipboardy": "2.0.0",
        "lodash": "^4.17.5",
        "make-dir": "3.0.0"
    },
    "lint-staged": {
        "**/*.js": [
            "prettier-eslint --prettier-last --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
